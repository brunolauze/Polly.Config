{
  "title": "Polly.Config",
  "version": "5.0.0.0",
  "dependencies": {
    "Microsoft.Extensions.Configuration": "1.0.0",
    "Microsoft.Extensions.Configuration.Json": "1.0.0",
    "NETStandard.Library": "1.6.0",
    "NuSpec.ReferenceGenerator": {
      "version": "1.4.0",
      "type": "build"
    },
    "Polly.Core": "5.0.0.0"
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true
      }
    }
  },
  "frameworks": {
    "netstandard1.6": {
      "imports": "dnxcore50"
    }
  },
  "buildOptions": {
    "outputName": "Polly.Config",
    "define": [ "SUPPORTS_ASYNC;PORTABLE" ],
    "compile": [ "..\\Polly.Config.Shared\\**\\*.cs" ]
  }
}
